# Basic integration test protocol involving only a call to a bash script that writes a single line to a text file.
#JSUB<summary-name> sample0009A
echo "Processing summary data from: "sample0009A
# root group 
OUTFILE="results/outPrefix_"sample0009A # Note: this is an example of what not to do! After the protocol is split OUTFILE will be undeclared in subsequent groups unless it is declared there again using variables from vars and fvars but NOT variables from the top of the script.
bash ../../bash_scripts/trivial.sh "$OUTFILE".txt
bash ../../bash_scripts/trivial.sh "$OUTFILE".txt
#JGROUP first
OUTFILE="results/outPrefix_"sample0009A 
bash ../../bash_scripts/concat.sh "$OUTFILE"_first.txt sub09X sub09Y
bash ../../bash_scripts/concat.sh "$OUTFILE"_first.txt sub09Y sub09Z
#JGROUP second
OUTFILE="results/outPrefix_"sample0009A 
bash ../../bash_scripts/concat.sh "$OUTFILE"_second.txt sub09Y sub09Z
bash ../../bash_scripts/concat.sh "$OUTFILE"_second.txt sub09Z sub09X
#JGROUP third
OUTFILE="results/outPrefix_"sample0009A 
bash ../../bash_scripts/concat.sh "$OUTFILE"_third.txt sub09Z sub09X
bash ../../bash_scripts/concat.sh "$OUTFILE"_third.txt sub09X sub09Y
#JGROUP last first second third
OUTFILE="results/outPrefix_"sample0009A
bash ../../bash_scripts/catfiles.sh "$OUTFILE".txt "$OUTFILE"_first.txt "$OUTFILE"_second.txt "$OUTFILE"_third.txt
