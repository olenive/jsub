# Basic integration test protocol involving only a call to a bash script that writes a single line to a text file.
#JSUB<summary-name> sample0001A
echo "Processing summary data from: "sample0001A
# root group 
OUTFILE="outPrefix_"sample0001A".txt"
bash ../../bash_scripts/trivial.sh "$OUTFILE"
bash ../../bash_scripts/trivial.sh "$OUTFILE"
#JGROUP first
bash ../../bash_scripts/concat.sh "$OUTFILE"_first sub01X sub01Y
bash ../../bash_scripts/concat.sh "$OUTFILE"_first sub01Y sub01Z
#JGROUP second
bash ../../bash_scripts/concat.sh "$OUTFILE"_second sub01Y sub01Z
bash ../../bash_scripts/concat.sh "$OUTFILE"_second sub01Z sub01X
#JGROUP third
bash ../../bash_scripts/concat.sh "$OUTFILE"_third sub01Z sub01X
bash ../../bash_scripts/concat.sh "$OUTFILE"_third sub01X sub01Y
#JGROUP last
bash ../../bash_scripts/catfiles.sh "$OUTFILE" "$OUTFILE"_first "$OUTFILE"_second "$OUTFILE"_third
